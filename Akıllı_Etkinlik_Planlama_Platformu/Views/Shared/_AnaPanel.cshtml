    <script>
        let currentPage = 1;
        const itemsPerPage = 4;

        // Sayfayı gösterme fonksiyonu
        function showPage(page) {
            const start = (page - 1) * itemsPerPage;
            const end = page * itemsPerPage;
            const items = document.querySelectorAll("#event-list .col-md-6");

            // Tüm etkinlikleri gizle
            items.forEach((item, index) => {
                item.style.display = "none";
            });

            // Belirli sayfada olan etkinlikleri göster
            for (let i = start; i < end && i < items.length; i++) {
                items[i].style.display = "block";
            }
        }

        // Sonraki sayfaya geçiş
        function nextPage() {
            if (currentPage < 3) { // Eğer sayfa 3'ten küçükse, bir sonraki sayfaya geç
                currentPage++;
                showPage(currentPage);
            }
        }

        // Önceki sayfaya geçiş
        function prevPage() {
            if (currentPage > 1) { // Eğer sayfa 1'den büyükse, bir önceki sayfaya geç
                currentPage--;
                showPage(currentPage);
            }
        }

        // İlk sayfayı göster
        showPage(currentPage);

        // Sayfa geçişlerini butonlara bağla
        document.getElementById("nextBtn").addEventListener("click", nextPage);
        document.getElementById("prevBtn").addEventListener("click", prevPage);
    </script>
