            });
        }
    </script>
    <script>
        function getDirectionss(destination) {
            var start = 'Kocaeli Üniversitesi Umuttepe Kampüsü';
            var end = destination;

            var mapContainer = document.getElementById('map');
            mapContainer.style.display = 'block'; // Harita görünür hale getiriliyor

            var map = new google.maps.Map(mapContainer, {
                zoom: 10,
                center: { lat: 40.7661, lng: 29.9299 }
            });

            var directionsService = new google.maps.DirectionsService();
            var directionsRenderer = new google.maps.DirectionsRenderer();
            directionsRenderer.setMap(map);

            var request = {
                origin: start,
                destination: end,
                travelMode: google.maps.TravelMode.DRIVING
            };

            directionsService.route(request, function (result, status) {
                if (status == 'OK') {
                    directionsRenderer.setDirections(result);
                } else {
                    alert('Yol tarifi alınırken bir hata oluştu: ' + status);
                }
            });
        }
    </script>
